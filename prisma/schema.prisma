generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Vehicle {
  v_id        Int     @id @default(autoincrement())
  name        String?
  type        String?
  price       Float?
  location    String?
  description String?
  imageUrl    String?
}
